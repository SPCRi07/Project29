<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.Project29" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/black</item>
        <item name="colorAccent">@color/black</item>
        <!-- Customize your theme here. -->
    </style>

    <style name="textlabel" parent="Widget.MaterialComponents.TextInputLayout.OutlinedBox">

        <item name="android:textColorHint">@color/black</item>
        <item name="colorAccent">@color/acc</item>
        <item name="colorControlNormal">@color/black</item>
        <item name="colorControlActivated">@color/acc</item>
        <item name="android:textColor">@color/black</item>
          <item name="boxStrokeColor">@color/acc</item>
    </style>


    <style name="appnametext" parent="Widget.MaterialComponents.TextView">

        <item name="android:fontFamily">@font/damion</item>
        <item name="android:textSize">30sp</item>
        <item name="android:text">@string/app_name</item>
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/black</item>

    </style>
    <style name="normaltext" parent="Widget.MaterialComponents.TextView">

        <item name="android:fontFamily">@font/adamina</item>
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/black</item>

    </style>



</resources>